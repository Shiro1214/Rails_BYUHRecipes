<% title "Login" %>
<%= bootstrap_form_with(url: sessions_path, method: :post) do |f| %>
    <%= f.email_field :email%>
    <%= f.password_field :password%>
    <%= f.primary 'Login'%>
<% end %>
<hr></hr>
<%= button_to "Login with Github", "/auth/github", class: "btn btn-info", data: {turbo: false} %>